    "@testing-library/react": "^16.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/rosie": "^0.0.45",
    "@typescript-eslint/eslint-plugin": "7.18.0",
