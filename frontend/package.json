{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@^1",
  "scripts": {
    "dev": "next dev --port 10001",
    "build:nextjs": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json",
    "lint:fix": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "fix": "bun prettier:write && bun lint:fix",
    "generate:factories": " ts-node --project ts-node.config.json generateFactories.ts",
    "generate:api": "TS_POST_PROCESS_FILE='./node_modules/.bin/prettier --write' openapi-generator-cli generate -i ../backend/swagger/v1/openapi.yaml -g typescript-fetch -o ./lib/api/generated-api-client --additional-properties=useSingleRequestParameter=false --openapi-normalizer REFACTOR_ALLOF_WITH_PROPERTIES_ONLY=true -t openapi-templates/typescript-fetch --enable-post-process-file && bun generate:factories",
    "check:unused": "depcheck"
  },
  "dependencies": {
