    "build:nextjs": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json",
    "lint:fix": "bun lint --fix",
    "prettier:write": "prettier --write --config .prettierrc .",
    "prettier:check": "prettier --check --config .prettierrc .",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "fix": "bun prettier:write && bun lint:fix",
    "generate:factories": " ts-node --project ts-node.config.json generateFactories.ts",
    "generate:api": "openapi-typescript ../backend/openapi/v1/openapi.yaml  -o ./lib/api/openapi-v1.d.ts && bun fix",
    "check:unused": "depcheck",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "vercel:pull": "vercel env pull .env.development.local"
