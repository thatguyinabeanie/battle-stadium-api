    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "framer-motion": "~11.3.31",
    "install": "^0.13.0",
    "intl-messageformat": "^10.5.14",
