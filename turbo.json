{
  "tasks": {

    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "rubocop": {
      "dependsOn": ["^rubocop"]
    },
    "eslint": {
      "dependsOn": ["^eslint"]
    },
    "bundle:install": {
      "dependsOn": ["^bundle:install"]
    }
  }
}
